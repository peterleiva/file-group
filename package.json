  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-webpack-plugin": "^2.5.2",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "commander": "^6.1.0",
    "pluralize": "^8.0.0"
