{
  "name": "file-group",
  "version": "0.4.1",
  "author": "Peter Leiva <pleiva@id.uff.br>",
  "license": "MIT",
  "description": "Tiny software for aggregating files into folders using filter and group naming critereas",
  "main": "dist/cli.js",
  "engines": {
    "node": ">=14.16.0"
  },
  "scripts": {
    "start": "node .",
    "build": "webpack --env mode=production",
    "dev": "npm run build -- --watch",
    "format": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "prettier": "prettier .",
    "test": "jest",
    "test:watch": "npm t -- --watch",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "file-group": "./dist/index.bundled.js"
  },
  "keywords": [
    "aggregator",
    "group file",
    "file system",
    "file"
  ],
  "repository": {
    "type": "url",
    "url": "https://github.com/peterleiva/group"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-webpack-plugin": "^2.5.2",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "commander": "^6.1.0",
    "pluralize": "^8.0.0"
  }
}
